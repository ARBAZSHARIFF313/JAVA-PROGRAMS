public class a10 {
    public static void main(String[] args) {

        int arr[]= {2, 7, 11, 15};

        for (int i = 0 ; i < arr.length ; i++) {
            for (int j = arr.length -1 ; j > 0 ; j--) {
                if (arr[i] + arr[j] == 9) {
                    System.out.print(i + "," + j);
                }
            }


        }
    }
}





