public class a8 {
    public static void main(String[] args) {
        int arr[] = {1,2,3,4,5};
        int r=2;

        for (int j = 0;j<r;j++) {
            int n = arr[0];


            for (int i = 0; i < arr.length - 1; i++) {
                arr[i] = arr[i + 1];
            }
            arr[arr.length - 1] = n;

        }

        for (int i = 0; i< arr.length;i++){
            System.out.print(arr[i]+" ");
        }
    }
}
